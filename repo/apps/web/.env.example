# Vision Platform Web App Environment Variables

# API Configuration
VITE_API_URL=http://localhost:3001
VITE_AI_SERVICE_URL=http://localhost:8000
VITE_WEBSOCKET_URL=ws://localhost:3001

# Authentication
VITE_GOOGLE_CLIENT_ID=your-google-oauth-client-id
VITE_MS_CLIENT_ID=your-microsoft-azure-client-id
VITE_MS_AUTHORITY=https://login.microsoftonline.com/common

# Payment Processing
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-publishable-key

# Feature Flags
VITE_ENABLE_REAL_TIME_ANALYTICS=true
VITE_ENABLE_MEDIA_PROCESSING=true
VITE_ENABLE_OFFLINE_MODE=false

# Development
VITE_DEBUG_MODE=true
VITE_LOG_LEVEL=info